<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		confirm=".handleGroupDialogConfirm"
		reset=".resetGroupDialog">
		<groupItems>
			<ViewSettingsItem text="{i18n>totalAmount}" key="totalAmount" />
			<ViewSettingsItem text="{i18n>orderStatus}" key="orderStatus" />
			<ViewSettingsItem text="{i18n>orderedBy}" key="orderedBy" />
		</groupItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>